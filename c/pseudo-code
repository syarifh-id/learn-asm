# menjumlahkan nilai dari 0 sampai 9

eax = 0
ecx = 0
for ecx = 0 , ecx < 10 :
	eax + ecx	
	ecx + 1
end for

--------------------------------------
# menjumlahkan nilai dari 0 sampai 9

eax = 0
ecx = 0
while ecx < 10 do :
	eax + ecx
	ecx + 1
end while

--------------------------------------
#menjumlahkan nilai dari 0 sampai 9 tetapi keluar dari loop ketika eax = 5

eax = 0
ecx = 0
for ecx 0 to 9 do:
	eax <- eax + ecx
	if eax >= 5	:
		break
end for
